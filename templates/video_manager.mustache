{{!
    @template mod_video/video_manager

    Template for displaying a video.

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Context variables required for this template:
    * uniqueid

    Example context (json):
    {
        "uniqueid": 123
    }
}}

{{> mod_video/video_manager_ui }}

{{#js}}
    require(["mod_video/video_manager"], function(VideoManager) {
        new VideoManager('{{uniqueid}}', { debug: true, onVideoSelect: console.log });
    });
{{/js}}

